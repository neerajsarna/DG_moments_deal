
subsection Numerical Constants
	set polynomial degree = 1
	set mapping order = 2
	# CAUTION refinement cycles = 1 for periodic square
	set total h refinement cycles = 1
	
	# global = 0
	# adaptive = 1
	# adaptive_kelly = 2
	set type of refinement = 0

	# type of assembly
	# meshworker = 0
	# manuel = 1
	set assembly type = 0
end


subsection Physical Constants
# constants corresponding to exact solution for system A
	set	tau = 0.1
	set	zeta = 1.0				
	set	chi = 1.0
	set	theta0 = 2.0
	set theta1 = 1.0
	set	uW = 0.1
	set	A0 = 0.0
	set	A1 = 0.2
	set	A2 = 0.1
	set kappa = 0.0
	set epsilon = 0.0
	set error_variable = rho
	set alpha = 0
end


subsection System Properties
	set total systems to load = 1
	# system_id = number of equations in case of un-regularized equations
	# system_id = -number of equations in case of regularized systems
	set system_id = 6
	set equations in the system = 6
	set nBC = 2

	# 0 for characteristic and 1 for odd
	set BC type = 0

	# 0 for symmetric and 1 for un symmetric
	set symmetric or un symmetric = 1

	# 0 for force of systemA,1
	# for force of systemB
	set force type = 0
end

subsection Mesh Info
	# 0 for ring and 1 for periodic square
	set mesh type = 0

	# 0 to read msh file and 1 for internal generation
	set mesh options = 1

	# left boundary of square domain
	set left boundary = -0.5
	set right boundary = 0.5
	set top boundary = 0.5
	set bottom boundary = -0.5
	set part_x = 2
	set part_y = 100
end

subsection Output Directory Name
	set Main Output Directory = output_comparision_Kn0p1_char
end
